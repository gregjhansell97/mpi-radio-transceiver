

# separatly copiling the cuda code into an object file
g++ -c ./cuda_wrapper_laptop.cpp -std=c++17

# linking the object file to the mpi compilation
mpic++ ./test_overflow.cpp ./radio_transceiver.cpp cuda_wrapper_laptop.o -o ./run.exe -std=c++17


mpirun -n 2 ./run.exe
rm ./run.exe
rm ./cuda_wrapper_laptop.o
